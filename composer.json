{
    "name": "widmogrod/zf2-mustache-module",
    "description": "Zend Framework 2 module providing integration with the Mustache rendering engine.",
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.3",
        "mustache/mustache": "2.*",
        "zendframework/zendframework-servicemanager": "2.*",
        "zendframework/zendframework-view": "2.*",
        "zendframework/zendframework-eventmanager": "2.*",
        "zendframework/zendframework-modulemanager": "2.*"
    },
    "authors": [
        {
            "name": "Gabriel Habryn",
            "email": "widmogrod@gmail.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "Mustache": "src"
        },
        "classmap": [
            "./Module.php"
        ]
    }
}
